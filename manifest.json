  
{
    "manifest_version": 2,
  
    "name": "Theme Changer",
    "description": "Change a website to different color themes",
    "version": "1.0.0",
    "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [{
      "js": ["popup.js"],
      "matches": ["http://*/*", "https://*/*"]
    }],
    "permissions": ["activeTab","storage"]
}